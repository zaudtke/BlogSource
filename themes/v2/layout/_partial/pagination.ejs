<nav>
    <ul class="pager">
        <% if (page.prev){ %>
            <li class="pager-newer">
                <i class="fa fa-chevron-left" aria-hidden="true"></i>
                <a href="<%- config.root %><%- page.prev_link %>">Newer</a>
            </li>
        <% } %>
        <% if (page.next){ %>
            <li class="pager-older">
                <a href="<%- config.root %><%- page.next_link %>">Older</a>
                <i class="fa fa-chevron-right" aria-hidden="true"></i>
            </li>
        <% } %>
    </ul>
</nav>